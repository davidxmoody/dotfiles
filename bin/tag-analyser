#!/usr/bin/env python3

from collections import Counter
import fileinput
import re

word_tallies = Counter()

for line in fileinput.input():
    tags = [tag.lower() for tag in re.findall(r'(#[A-Za-z0-9_-]+)', line)]
    word_tallies.update(tags)

for word, count in word_tallies.most_common(2000):
    print(count, word)
